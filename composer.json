{
    "name": "itsgoingd/clockwork",
    "description": "Server-side component of Clockwork, a Chrome extension for PHP development",
    "keywords": ["debugging", "profiling", "logging", "laravel", "lumen", "slim", "devtools"],
    "homepage": "https://underground.works/clockwork",
    "license": "MIT",
    "authors": [
        {
            "name": "itsgoingd",
            "email": "itsgoingd@luzer.sk",
            "homepage": "https://twitter.com/itsgoingd"
        }
    ],
    "require": {
        "php": ">=5.4",
        "psr/log": "1.*"
    },
    "autoload": {
        "psr-4": {
            "Clockwork\\": "Clockwork/"
        }
    },
    "extra": {
        "laravel": {
            "providers": [
                "Clockwork\\Support\\Laravel\\ClockworkServiceProvider"
            ],
            "aliases": {
                "Clockwork": "Clockwork\\Support\\Laravel\\Facade"
            }
        }
    }
}
